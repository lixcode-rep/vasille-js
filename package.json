{
  "name": "vasille-js",
  "version": "1.2.3",
  "description": "The fastest (Vanilla.js based) front-end javascript framework",
  "main": "src/index.js",
  "types": "types/index.d.ts",
  "exports": {
    "import": "./src/index.js",
    "browser": "./src/index.js"
  },
  "scripts": {
    "flow": "flow",
    "prebuild": "rm -r lib",
    "build": "flow-remove-types --pretty --sourcemaps src/ -d lib/",
    "prepare": "flow-remove-types --pretty --sourcemaps src/ -d lib/",
    "prepack": "tsc --build tsconfig.json",
    "test": "echo \"Error: no test specified\"",
    "predoc": "npm run build",
    "doc": "jsdoc -a all lib/** DOCHOME.md",
    "tsc": "tsc --build tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/vasille.js/vasille.js.git"
  },
  "keywords": [
    "javascript",
    "framework",
    "frontend",
    "fast"
  ],
  "author": "Vasile Lixcode <lixcode@vivaldi.net> (https://t.me/lixcode)",
  "license": "MIT",
  "devDependencies": {
    "@types/debug": "latest",
    "@types/eslint": "latest",
    "@types/eslint-scope": "latest",
    "@types/estree": "latest",
    "@types/events": "latest",
    "@types/node": "latest",
    "flow-bin": "latest",
    "flow-remove-types": "latest",
    "jsdoc": "latest",
    "typescript": "latest"
  }
}
